{
  "name": "npec",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "precommit": "lint-staged",
    "eslint-check": "eslint --print-config .eslintrc.json | eslint-config-prettier-check",
    "eslint": "eslint --fix src/**/*.js",
    "generate-docs": "jsdoc -c jsdoc.json",
    "build-css": "node-sass-chokidar src/styles/scss/app.scss -o src/styles/css",
    "watch-css": "npm run build-css && node-sass-chokidar src/styles/scss -o src/styles/css --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "ag-grid": "^13.3.1",
    "ag-grid-react": "^13.3.0",
    "axios": "^0.16.2",
    "bootstrap": "^4.0.0-alpha.6",
    "classnames": "^2.2.5",
    "create-react-class": "^15.6.0",
    "font-awesome": "^4.7.0",
    "grommet": "^1.6.0",
    "immutability-helper": "^2.3.1",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.0.2",
    "popper.js": "^1.12.5",
    "prop-types": "^15.5.10",
    "react": "^15.6.2",
    "react-bootstrap": "^0.31.2",
    "react-color": "2.13.5",
    "react-contexify": "2.0.7",
    "react-data-grid": "^2.0.55",
    "react-dom": "^15.6.2",
    "react-dom-factories": "^1.0.1",
    "react-redux": "^5.0.6",
    "react-scripts": "1.0.10",
    "react-select": "^1.0.0-rc.10",
    "react-sortable-tree": "^0.1.21",
    "react-table": "^6.5.3",
    "react-transition-group": "^1.2.0",
    "reactcss": "^1.2.2",
    "reactstrap": "^4.8.0",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "docdash": "^0.4.0",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^3.10.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-flowtype": "^2.37.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "jsdoc": "~3.5.5",
    "lint-staged": "^4.2.3",
    "moxios": "^0.4.0",
    "prettier": "^1.7.3",
    "react-test-renderer": "^15.6.1",
    "redux-devtools-extension": "^2.13.2",
    "redux-mock-store": "^1.3.0",
    "sinon": "^3.2.1",
    "standard": "^10.0.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js",
      "!src/registerServiceWorker.js",
      "!src/store/*.js",
      "!src/mockData/*.js",
      "!src/services/constants.js",
      "!src/actions/index.js",
      "!src/actions/types.js",
      "!src/reducers/index.js",
      "!src/tests/**/*.js",
      "!src/components/treeView/treeNodeRenderer.js"
    ]
  },
  "lint-staged": {
    "gitDir": "../../",
    "src/**/*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
